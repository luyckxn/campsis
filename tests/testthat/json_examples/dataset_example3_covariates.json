[
  {
    "type": "arm",
    "list": [
      {
        "type": "arm_attributes",
        "subjects": 100,
        "label": "My dataset"
      },
      {
        "type": "observations",
        "times": {
          "type": "time_sequence",
          "start": 0.0,
          "end": 24.0,
          "by": 1.0
        }
      },
      {
        "type": "covariate",
        "name": "BW1",
        "distribution": {
          "type": "constant_distribution",
          "value": 70.5
        }
      },
      {
        "type": "covariate",
        "name": "BW2",
        "distribution": {
          "type": "fixed_distribution",
          "values": [70.5, 80.5, 90.5]
        }
      },
      {
        "type": "covariate",
        "name": "BW3",
        "distribution": {
          "type": "normal_distribution",
